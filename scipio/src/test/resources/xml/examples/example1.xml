<?xml version="1.0" encoding="UTF-8"?>
<transaction>
	<date>${tx.date}</date>
	<time>${tx.time}</time>
	<application-id>${app.id}</application-id>
	<user>
		<name>${user.name}</name>
	</user>
	<merchant>
		<smi>${merchant.smi}</smi>
		<name>${merchant.name}</name>
		<store>
			<number>${merchant.store.number}</number>
		</store>
		<telephone>${merchant.telephone}</telephone>
		<register>
			<number>${register.number}</number>
		</register>
		<address>
			<street>${merchant.address.street}</street>
			<city>${merchant.address.city}</city>
			<state>${merchant.address.state}</state>
			<postal-code>${merchant.address.zip}</postal-code>
			<country>${merchant.address.country}</country>
		</address>
	</merchant>
	<consumer>
		<pei>${consumer.pei}</pei>
		<tei>${consumer.tei}</tei>
		<pin>${consumer.pin}</pin>
		<name>
			<first>${consumer.name.first}</first>
			<last>${consumer.name.last}</last>
		</name>
		<card>
			<type>${consumer.card.type</type>
			<last4>${consumer.card.last4</last4>
			<expiration>
				<month>${consumer.card.exp.month}</month>
				<year>${consumer.card.exp.year}</year>
			</expiration>
		<card>
	</consumer>
	<receipt>
		<number>${receipt.number}</number>
		<items>
			<item>
				<description>${receipt.items[1].description}</description>
				<quantity>${receipt.items[1].qty}</quantity>
				<unit-price>${receipt.items[1].price}</unit-price>
				<inventory-number>${receipt.items[1].inventorynumber}</inventory-number>
			</item>
			<item>
				<description>${receipt.items[2].description}</description>
				<quantity>${receipt.items[2].qty}</quantity>
				<unit-price>${receipt.items[2].price}</unit-price>
				<inventory-number>${receipt.items[2].inventorynumber}</inventory-number>
			</item>
		</items>
		<subtotal>${receipt.subtotal}</subtotal>
		<tip>${receipt.tip}</tip>
		<tax>${receipt.tax}</tax>
		<total>${receipt.total}</total>
		<total-items>${receipt.items.count}</total-items>
	</receipt>
	<coupon>
		<name>${coupon.name}</name>
		<number>${coupon.number}</number>
	</coupon>
</transaction>

		

